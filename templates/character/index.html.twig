{% extends 'base.html.twig' %}

{% block title %}Listes des personnages{% endblock %}

{% block body %}

    <h1>Listes des personnages</h1>

    {% for character in characters %}
        <a href="/character/{{ character.id }}">{{ character.name }} {{ character.fightingStyle }} <img src="{{character.profilPhoto}}"><br></a>
    {% endfor %}

{% endblock %}
